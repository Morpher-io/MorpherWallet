{
  "name": "morpher-wallet-backend",
  "version": "1.0.0",
  "description": "Morpher Wallet Backend",
  "main": "index.ts",
  "scripts": {
    "test": "mocha -r ts-node/register src/tests/*.test.ts --exit",
    "start": "ts-node ./src/index.ts",
    "start:production": "./node_modules/pm2/bin/pm2-runtime index.js --max-memory-restart 2G",
    "start:watch": "nodemon --exec ts-node ./src/index.ts",
    "start:debug": "nodemon --inspect ts-node ./src/index.ts",
    "db:seed": "ts-node ./src/database/seed/index.ts",
    "env:dev": "ts-node src/helpers/functions/aws.ts dev",
    "env:production": "ts-node src/helpers/functions/aws.ts production",
    "env:staging": "ts-node src/helpers/functions/aws.ts staging",
    "lint": "eslint --ext .ts,.js ./src",
    "lint-fix": "eslint --ext .ts,.js ./src --fix",
    "format": "prettier --check './src/**/*.*'",
    "format-fix": "prettier --write './src/**/*.*'",
    "build": "tsc"
  },
  "author": "Morpher Team",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "await-to-js": "^3.0.0",
    "aws-sdk": "^2.1107.0",
    "axios": "^0.26.1",
    "bad-words": "^3.0.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dataloader-sequelize": "^2.3.3",
    "dotenv": "^16.0.0",
    "email-domain-check": "^1.1.4",
    "email-validator": "^2.0.4",
    "ethereumjs-util": "^7.1.4",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "fb": "^2.0.0",
    "googleapis": "^100.0.0",
    "helmet": "^5.0.2",
    "isomorphic-fetch": "^3.0.0",
    "mailchecker": "^4.1.14",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "otplib": "^12.0.1",
    "password-validator": "^5.2.1",
    "pg": "^8.7.3",
    "prettier": "^2.6.2",
    "qrcode": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.18.0",
    "sequelize-typescript": "^2.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "validate.js": "^0.13.1",
    "vk-io": "^4.5.2",
    "web3-eth-accounts": "^1.7.1",
    "winston": "^3.7.2",
    "winston-aws-cloudwatch": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^1.7.3",
    "@types/mocha": "^9.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@types/sequelize": "^4.28.11",
    "@types/validator": "13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "cypress-otp": "^1.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "expect": "^27.5.1",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
  }
}
