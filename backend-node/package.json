{
  "name": "morpher-wallet-backend",
  "version": "1.0.0",
  "description": "Morpher Wallet Backend",
  "main": "index.ts",
  "scripts": {
    "test": "mocha -r ts-node/register src/tests/*.test.ts --exit",
    "start": "ts-node ./src/index.ts",
    "start:production": "./node_modules/pm2/bin/pm2-runtime index.js --max-memory-restart 2G",
    "start:watch": "nodemon --exec ts-node ./src/index.ts",
    "start:debug": "nodemon --inspect ts-node ./src/index.ts",
    "db:seed": "ts-node ./src/database/seed/index.ts",
    "env:dev": "ts-node src/helpers/functions/aws.ts dev",
    "env:production": "ts-node src/helpers/functions/aws.ts production",
    "env:staging": "ts-node src/helpers/functions/aws.ts staging",
    "lint": "eslint --ext .ts,.js ./src",
    "lint-fix": "eslint --ext .ts,.js ./src --fix",
    "format": "prettier --check './src/**/*.*'",
    "format-fix": "prettier --write './src/**/*.*'",
    "build": "tsc"
  },
  "author": "Morpher Team",
  "license": "ISC",
  "private": true,
  "dependencies": {
    "await-to-js": "^2.1.1",
    "aws-sdk": "^2.788.0",
    "axios": "^0.22.0",
    "bad-words": "^3.0.3",
    "cors": "^2.8.5",
    "cron": "^1.6.0",
    "dataloader-sequelize": "^1.7.3",
    "dotenv": "^6.1.0",
    "email-domain-check": "^1.1.4",
    "email-validator": "^2.0.4",
    "ethereumjs-util": "^7.0.8",
    "express": "^4.16.3",
    "express-rate-limit": "^3.5.1",
    "facebook-nodejs-business-sdk": "^7.0.3",
    "fb": "^2.0.0",
    "googleapis": "^58.0.0",
    "helmet": "^3.15.0",
    "isomorphic-fetch": "^3.0.0",
    "mailchecker": "^3.3.14",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "otplib": "^12.0.1",
    "password-validator": "^5.1.0",
    "pg": "^7.7.1",
    "prettier": "^1.19.1",
    "qrcode": "^1.5.0",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^6.12.1",
    "sequelize-typescript": "^2.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.6.3",
    "validate.js": "^0.13.1",
    "vk-io": "^4.0.0-rc.30",
    "web3-eth-accounts": "^1.5.2",
    "winston": "^3.1.0",
    "winston-aws-cloudwatch": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cron": "^1.7.1",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/node": "^10.12.18",
    "@types/sequelize": "^4.28.8",
    "@types/validator": "10.11.3",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "cypress-otp": "^1.0.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "expect": "^27.4.2",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.13",
    "puppeteer": "^10.2.0",
    "supertest": "^3.4.2"
  }
}
